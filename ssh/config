ControlMaster auto
ControlPath /tmp/%r@%h:%p
ControlPersist yes
Host *
    ServerAliveInterval 240
    HostKeyAlgorithms +ssh-rsa
    PubkeyAcceptedKeyTypes +ssh-rsa
Host github.com
    IdentityFile ~/.ssh/id_github.com
Host *.coding.net
    IdentityFile ~/.ssh/id_coding.net
Host linux-kernel-dev
    IdentityFile ~/.ssh/id_linux_kernel_dev
